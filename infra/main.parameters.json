{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "telemetryEnabled": {
      "value": "true",
      "metadata": {
        "description": "Set to true to support the project and report the deployment. This will be written to the env file as TELEMETRY_ENABLED"
      }
    }
  }
}
